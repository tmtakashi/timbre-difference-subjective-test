  "build": {
    "appId": "com.electron.yourapp",
    "directories": {
      "output": "dist"
    },
    "files": [
      "./dist/**/*.js",
      "./dist/**/*.js.map",
      "./dist/index.html",
      "./dist/index.css",
      "./dist/assets/**/*",
      "./index.html",
      "./src/main.ts"
    ],
    "extraResources": [
      {
        "from": "./dist/assets",
        "to": "assets"
      }
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
